{
  "app": {
    "name": "FAMO TrafficApp",
    "version": "3.0.0",
    "environment": "development"
  },
  "database": {
    "type": "sqlite",
    "path": "data/traffic.db",
    "schema": "public"
  },
  "geocoding": {
    "provider": "nominatim",
    "base_url": "https://nominatim.openstreetmap.org/search",
    "rate_limit": 1,
    "timeout": 20,
    "contact_email": ""
  },
  "routing": {
    "max_tour_duration_minutes": 60,
    "service_time_per_customer_minutes": 2,
    "depot_coordinates": {
      "lat": 51.0500,
      "lon": 13.7373,
      "address": "Stuttgarter Str. 33, 01189 Dresden"
    }
  },
  "ai": {
    "provider": "openai",
    "model": "gpt-4o-mini",
    "max_tokens": 4000,
    "temperature": 0.1
  },
  "logging": {
    "level": "INFO",
    "access_level": "WARNING",
    "file_path": "logs/trafficapp.log"
  },
  "paths": {
    "original_csvs": "./Tourplaene",
    "staging": "./data/staging",
    "output": "./data/output",
    "backup": "./routen"
  }
}
